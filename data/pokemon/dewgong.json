  "pokedex_entries": {
    "Red": {
      "en": "Stores thermal energy in its body. Swims at a steady 8 knots even in intensely cold waters.",
      "de": "Der Körper dieses Pokémon speichert Wärme. Selbst in eiskaltem Wasser schwimmt es acht Knoten schnell."
    },
    "Blue": {
      "en": "Stores thermal energy in its body. Swims at a steady 8 knots even in intensely cold waters.",
      "de": "Der Körper dieses Pokémon speichert Wärme. Selbst in eiskaltem Wasser schwimmt es acht Knoten schnell."
    },
    "Yellow": {
      "en": "Its entire body is a snowy-white. Unharmed by even intense cold, it swims powerfully in icy waters.",
      "de": "Der Körper dieses Pokémon ist schneeweiß. Es kann sogar in Eiswasser kraftvoll schwimmen."
    },
    "Gold": {
      "en": "Its streamlined body has little drag in water. The colder the temperature, the friskier it gets.",
      "de": "Es verspürt kaum Widerstand im Wasser. Je niedriger die Temperatur, desto verspielter verhält es sich."
    },
    "Silver": {
      "en": "It loves frigid seas with ice floes. It uses its long tail to change swimming direction quickly.",
      "de": "Es liebt das Eismeer. Beim Schwimmen setzt es seinen langen Schwanz ein, um die Richtung zu ändern."
    },
    "Crystal": {
      "en": "It sleeps under shallow ocean waters during the day, then looks for food at night when it's cold.",
      "de": "Tagsüber schläft es in niedrigen Meerestiefen, Nachts, wenn es kalt ist, sucht es nach Nahrung."
    },
    "Ruby": {
      "en": "Dewgong loves to snooze on bitterly cold ice. The sight of this Pokémon sleeping on a glacier was mistakenly thought to be a mermaid by a mariner long ago.",
