  "pokedex_entries": {
    "Black": {
      "en": "The temperature of their breath is -58° F. They create snow crystals and make snow fall in the areas around them.",
      "de": "Die Temperatur seines Odems liegt bei minus 50°C. Es erzeugt Eiskristalle und lässt es in seiner Umgebung schneien."
    },
    "White": {
      "en": "This Pokémon formed from icicles bathed in energy from the morning sun. It sleeps buried in snow.",
      "de": "Ein Eiszapfen, der durch die Energie der aufgehenden Sonne zum Pokémon wurde. Nachts deckt es sich mit Schnee zu."
    },
    "Black 2": {
      "en": "Theoretically, this Pokémon formed from icicles bathed in energy from the morning sun. Their breath is -58° F.",
      "de": "Ein Eiszapfen, der durch die Energie der aufgehenden Sonne zum Pokémon wurde. Es speit minus 50 Grad kalten Odem."
    },
    "White 2": {
      "en": "Theoretically, this Pokémon formed from icicles bathed in energy from the morning sun. Their breath is -58° F.",
      "de": "Ein Eiszapfen, der durch die Energie der aufgehenden Sonne zum Pokémon wurde. Es speit minus 50 Grad kalten Odem."
    },
    "X": {
      "en": "The temperature of their breath is -58 degrees Fahrenheit. They create snow crystals and make snow fall in the areas around them.",
      "de": "Die Temperatur seines Odems liegt bei -50 °C. Es erzeugt Eiskristalle und lässt es in seiner Umgebung schneien."
    },
    "Y": {
      "en": "This Pokémon formed from icicles bathed in energy from the morning sun. It sleeps buried in snow.",
      "de": "Ein Eiszapfen, der durch die Energie der aufgehenden Sonne zum Pokémon wurde. Nachts deckt es sich mit Schnee zu."
    },
    "Omega Ruby": {
      "en": "The temperature of their breath is -58 degrees Fahrenheit. They create snow crystals and make snow fall in the areas around them.",
      "de": "Ein Eiszapfen, der durch die Energie der aufgehenden Sonne zum Pokémon wurde. Nachts deckt es sich mit Schnee zu."
    },
    "Alpha Sapphire": {
      "en": "This Pokémon formed from icicles bathed in energy from the morning sun. It sleeps buried in snow.",
      "de": "Ein Eiszapfen, der durch die Energie der aufgehenden Sonne zum Pokémon wurde. Nachts deckt es sich mit Schnee zu."
    },
    "Sun": {
      "en": "Born of an icicle, this Pokémon uses its frosty breath to make ice crystals, causing snow to fall.",
      "de": "Dieses Pokémon ist aus einem Eiszapfen entstanden. Mit seinem kalten Odem erzeugt es Eiskristalle und lässt es schneien."
    },
    "Moon": {
      "en": "It feels pleasantly cool when embraced. This Pokémon is treasured by households in warm regions.",
      "de": "Wenn man es umarmt, fühlt es sich angenehm und kühl an. In heißen Regionen hat man daher gern ein Exemplar im Haus."
    },
    "Ultra Sun": {
      "en": "It exhales cold air that is at –58 degrees Fahrenheit. When it’s in a warm place, it shrinks little by little.",
      "de": "Die Temperatur seines Atems liegt bei -50 °C. Wenn es sich an warmen Orten aufhält, wird es nach und nach kleiner."
    },
    "Ultra Moon": {
      "en": "When the morning sun hit an icicle, it wished not to melt, and thus Vanillite was born. At night, it buries itself in snow to sleep.",
      "de": "Es entstand aus dem Wunsch eines Eiszapfens, der in der Morgensonne nicht schmelzen wollte. Nachts schläft es tief vergraben im Schnee."
    }
  },
