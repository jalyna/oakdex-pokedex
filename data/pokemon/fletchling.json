  "pokedex_entries": {
    "X": {
      "en": "These friendly Pokémon send signals to one another with beautiful chirps and tail-feather movements.",
      "de": "Ein sehr zutrauliches Pokémon. Durch Zwitschern und Bewegen der Schwanzfedern sendet es Signale an seine Gefährten."
    },
    "Y": {
      "en": "Despite the beauty of its lilting voice, it's merciless to intruders that enter its territory.",
      "de": "Sein Zwitschern ist wunderschön, aber wenn Gegner sein Revier betreten, kennt es keine Gnade."
    },
    "Omega Ruby": {
      "en": "These friendly Pokémon send signals to one another with beautiful chirps and tail-feather movements.",
      "de": "Sein Zwitschern ist wunderschön, aber wenn Gegner sein Revier betreten, kennt es keine Gnade."
    },
    "Alpha Sapphire": {
      "en": "Despite the beauty of its lilting voice, it's merciless to intruders that enter its territory.",
      "de": "Sein Zwitschern ist wunderschön, aber wenn Gegner sein Revier betreten, kennt es keine Gnade."
    },
    "Sun": {
      "en": "This amiable Pokémon is easy to train. But when battle is joined, it shows its ferocious side.",
      "de": "Ein sehr zutrauliches und pflegeleichtes Pokémon, das im Kampf jedoch erstaunlich wild und ungestüm agiert."
    },
    "Moon": {
      "en": "When it's excited, its temperature can double, spiking hormone production in its body.",
      "de": "Ist es aufgeregt, steigt seine Körpertemperatur auf das Doppelte an und seine Hormone spielen verrückt."
    },
    "Ultra Sun": {
      "en": "While it’s an amiable Pokémon, if you touch it unexpectedly, it will heat up its body in an instant, and you’ll be burned.",
      "de": "Es ist eigentlich sehr zutraulich. Berührt man es aber gegen seinen Willen, erhitzt es blitzartig seinen Körper, was zu Verbrennungen führt."
    },
    "Ultra Moon": {
      "en": "Its body is always warm. Trainers who live in cold areas apparently sleep with it in their bed.",
      "de": "Sein Körper ist immer warm. In kalten Gebieten schlafen Trainer deshalb oft in einem Bett mit ihm."
    }
  },
