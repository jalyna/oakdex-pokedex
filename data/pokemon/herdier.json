  "pokedex_entries": {
    "Black": {
      "en": "It has black, cape-like fur that is very hard and decreases the amount of damage it receives.",
      "de": "Das dunkle Fell, das es wie ein Mantel umgibt, ist äußerst widerstandsfähig und mindert den Schaden durch Attacken."
    },
    "White": {
      "en": "It loyally follows its Trainer's orders. For ages, they have helped Trainers raise Pokémon.",
      "de": "Folgt treu den Befehlen seines Trainers. Schon seit jeher ist es als rechte Hand der Pokémon-Trainer bekannt."
    },
    "Black 2": {
      "en": "This very loyal Pokémon helps Trainers, and it also takes care of other Pokémon.",
      "de": "Dieses äußerst treue Pokémon geht nicht nur seinem Trainer zur Hand, sondern hilft auch anderen Pokémon."
    },
    "White 2": {
      "en": "This very loyal Pokémon helps Trainers, and it also takes care of other Pokémon.",
      "de": "Dieses äußerst treue Pokémon geht nicht nur seinem Trainer zur Hand, sondern hilft auch anderen Pokémon."
    },
    "X": {
      "en": "This very loyal Pokémon helps Trainers, and it also takes care of other Pokémon.",
      "de": "Dieses äußerst treue Pokémon geht nicht nur seinem Trainer zur Hand, sondern hilft auch anderen Pokémon."
    },
    "Y": {
      "en": "It has black, cape-like fur that is very hard and decreases the amount of damage it receives.",
      "de": "Das dunkle Fell, das es wie ein Mantel umgibt, ist äußerst widerstandsfähig und mindert den Schaden durch Attacken."
    },
    "Omega Ruby": {
      "en": "This very loyal Pokémon helps Trainers, and it also takes care of other Pokémon.",
      "de": "Das dunkle Fell, das es wie ein Mantel umgibt, ist äußerst widerstandsfähig und mindert den Schaden durch Attacken."
    },
    "Alpha Sapphire": {
      "en": "It has black, cape-like fur that is very hard and decreases the amount of damage it receives.",
      "de": "Das dunkle Fell, das es wie ein Mantel umgibt, ist äußerst widerstandsfähig und mindert den Schaden durch Attacken."
    },
    "Sun": {
      "en": "Its dense black fur grows continuously. The high cost of keeping its hard fur properly groomed makes this a troublesome Pokémon to train.",
      "de": "Sein dunkles Fell ist sehr widerstandsfähig und wächst schnell. Dadurch sind die Kosten für das Trimmen sehr hoch."
    },
    "Moon": {
      "en": "This Pokémon obeys its master's orders faithfully. However, it refuses to listen to anything said by a person it doesn't respect.",
      "de": "Es folgt geflissentlich den Befehlen seines Trainers. Auf jemanden, den es als unterlegen betrachtet, hört es jedoch nicht."
    },
    "Ultra Sun": {
      "en": "The longer its black fur grows, the harder and more impervious it gets. Claws and fangs can’t easily penetrate it.",
      "de": "Mit zunehmendem Alter wird sein dunkles Fell immer widerstandsfähiger, sodass selbst Klauen und Fangzähne es kaum durchdringen können."
    },
    "Ultra Moon": {
      "en": "It has been living with people for so long that portrayals of it can be found on the walls of caves from long, long ago.",
      "de": "Uralte Wandmalereien sprechen dafür, dass Terribark schon seit langer Zeit an der Seite der Menschen lebt."
    }
  },
