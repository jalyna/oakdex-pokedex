  "pokedex_entries": {
    "X": {
      "en": "It sends a soothing aura from its ribbonlike feelers to calm fights.",
      "de": "Seinen bandförmigen Fühlern lässt es beruhigende Wellen entströmen, die das Ende von Kämpfen herbeiführen."
    },
    "Y": {
      "en": "It wraps its ribbonlike feelers around the arm of its beloved Trainer and walks with him or her.",
      "de": "Es umwickelt den Arm seines geliebten Trainers mit seinen bandförmigen Fühlern und geht so mit ihm spazieren."
    },
    "Omega Ruby": {
      "en": "It sends a soothing aura from its ribbonlike feelers to calm fights.",
      "de": "Es umwickelt den Arm seines geliebten Trainers mit seinen bandförmigen Fühlern und geht so mit ihm spazieren."
    },
    "Alpha Sapphire": {
      "en": "It wraps its ribbonlike feelers around the arm of its beloved Trainer and walks with him or her.",
      "de": "Es umwickelt den Arm seines geliebten Trainers mit seinen bandförmigen Fühlern und geht so mit ihm spazieren."
    },
    "Sun": {
      "en": "Its ribbonlike feelers give off an aura that weakens hostility in its prey, causing them to let down their guard. Then it attacks.",
      "de": "Seinen bandförmigen Fühlern lässt es beruhigende Wellen entströmen, um seine Beute in Sicherheit zu wiegen."
    },
    "Moon": {
      "en": "When this Pokémon sights its prey, it swirls its ribbonlike feelers as a distraction. A moment later, it pounces.",
      "de": "Hat es Beute entdeckt, wedelt es mit seinen bandförmigen Fühlern, um diese abzulenken. Bei Erfolg stürzt es sich auf sie."
    },
    "Ultra Sun": {
      "en": "Once a fight breaks out, it will unflinchingly charge at dragon Pokémon that are many times larger than itself.",
      "de": "Kommt es zum Kampf, stürzt es sich furchtlos ins Gefecht, selbst wenn sein Gegner ein vielfach größeres Drachen-Pokémon ist."
    },
    "Ultra Moon": {
      "en": "Sylveon wraps its ribbonlike feelers around its Trainer’s arm because this touch enables it to read its Trainer’s feelings.",
      "de": "Feelinara umwickelt den Arm seines Trainers mit seinen bandförmigen Fühlern, da es so spüren kann, wie es ihm geht."
    }
  },
