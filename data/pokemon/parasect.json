  "pokedex_entries": {
    "Red": {
      "en": "A host-parasite pair in which the parasite mushroom has taken over the host bug. Prefers damp places.",
      "de": "Dieses Pokémon besteht aus einem Wirt und einem Parasiten. Es bevorzugt feuchte Umgebungen."
    },
    "Blue": {
      "en": "A host-parasite pair in which the parasite mushroom has taken over the host bug. Prefers damp places.",
      "de": "Dieses Pokémon besteht aus einem Wirt und einem Parasiten. Es bevorzugt feuchte Umgebungen."
    },
    "Yellow": {
      "en": "The bug host is drained of energy by the mushrooms on its back. They appear to do all the thinking.",
      "de": "Der Pilz auf dem Rücken entzieht dem Wirts-Pokémon Energie. Er hat die Kontrolle über das Pokémon."
    },
    "Gold": {
      "en": "It stays mostly in dark, damp places, the preference not of the bug, but of the big mushrooms on its back.",
      "de": "Es hält sich meist an dunklen und feuchten Orten auf. Der Riesenpilz auf seinem Rücken mag das."
    },
    "Silver": {
      "en": "The larger the mushroom on its back grows, the stronger the mushroom spores it scatters.",
      "de": "Je größer der Pilz auf seinem Rücken wird, desto stärker werden auch die Sporen, die es verteilt."
    },
    "Crystal": {
      "en": "When nothing's left to extract from the bug, the mushrooms on its back leave spores on the bug's egg.",
      "de": "Kann der Pilz auf seinem Rücken dem Käfer nichts mehr entziehen, übersät er dessen Ei mit Sporen."
    },
    "Ruby": {
      "en": "Parasect is known to infest large trees en masse and drain nutrients from the lower trunk and roots. When an infested tree dies, they move onto another tree all at once.",
      "de": "Parasek sucht große Bäume heim und entzieht Nährstoffe aus Stamm und Wurzeln. Wenn ein befallener Baum stirbt, sucht es sich zusammen mit seinen Artgenossen einen neuen Wirt."
    },
    "Sapphire": {
      "en": "Parasect is known to infest large trees en masse and drain nutrients from the lower trunk and roots. When an infested tree dies, they move onto another tree all at once.",
      "de": "Parasek sucht große Bäume heim und entzieht Nährstoffe aus Stamm und Wurzeln. Wenn ein befallener Baum stirbt, sucht es sich zusammen mit seinen Artgenossen einen neuen Wirt."
    },
    "Emerald": {
      "en": "Parasect are known to infest the roots of large trees en masse and drain nutrients. When an infested tree dies, they move onto another tree all at once.",
      "de": "PARASEK sucht große Bäume heim und ent zieht ihnen Nährstoffe. Stirbt ein befallener Baum, sucht es sich zusammen mit seinen Artgenossen einen neuen Wirt."
    },
    "FireRed": {
      "en": "It scatters toxic spores from the mushroom cap. In China, the spores are used as herbal medicine.",
      "de": "Es verstreut giftige Sporen aus seinem Pilzhut. In China wird aus den Sporen Kräutermedizin gewonnen."
    },
    "LeafGreen": {
      "en": "A host-parasite pair in which the parasite mushroom has taken over the host bug. Prefers damp places.",
      "de": "Dieses Pokémon besteht aus einem Wirt und einem Parasiten. Es bevorzugt feuchte Umgebungen."
    },
    "Diamond": {
      "en": "A mushroom grown larger than the host's body controls Parasect. It scatters poisonous spores.",
      "de": "PARASEK wird von einem Pilz, der größer als das Pokémon ist, kontrolliert. Er gibt Giftsporen ab."
    },
    "Pearl": {
      "en": "It is controlled by a mushroom grown larger than the bug body. It is said to prefer damp places.",
      "de": "Es wird von einem Pilz, der größer als sein Käferkörper ist, kontrol- liert. Es bevorzugt feuchte Orte."
    },
    "Platinum": {
      "en": "A mushroom grown larger than the host's body controls Parasect. It scatters poisonous spores.",
      "de": "PARASEK wird von einem Pilz, der größer als das Pokémon ist, kontrolliert. Er gibt Giftsporen ab."
