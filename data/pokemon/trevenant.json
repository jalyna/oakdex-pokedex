  "pokedex_entries": {
    "X": {
      "en": "It can control trees at will. It will trap people who harm the forest, so they can never leave.",
      "de": "Die Bäume des Waldes folgen ihm blind. Menschen, die dem Wald Schaden zufügen, hält es bis an ihr Lebensende darin gefangen."
    },
    "Y": {
      "en": "Using its roots as a nervous system, it controls the trees in the forest. It's kind to the Pokémon that reside in its body.",
      "de": "Es nutzt seine Wurzeln als Nervensystem und kontrolliert so andere Bäume. Es ist nett zu den Pokémon, die in ihm leben."
    },
    "Omega Ruby": {
      "en": "It can control trees at will. It will trap people who harm the forest, so they can never leave.",
      "de": "Es nutzt seine Wurzeln als Nervensystem und kontrolliert so andere Bäume. Es ist nett zu den Pokémon, die in ihm leben."
    },
    "Alpha Sapphire": {
      "en": "Using its roots as a nervous system, it controls the trees in the forest. It's kind to the Pokémon that reside in its body.",
      "de": "Es nutzt seine Wurzeln als Nervensystem und kontrolliert so andere Bäume. Es ist nett zu den Pokémon, die in ihm leben."
    },
    "Sun": {
      "en": "This Pokémon is said to devour anyone daring to ravage the forest. To the creatures dwelling in the forest, it offers great kindness.",
      "de": "Kein Eintrag vorhanden"
    },
    "Moon": {
      "en": "Through its roots, it exerts control over other trees. A deadly curse falls upon anyone cutting down trees in forests where Trevenant dwell.",
      "de": "Kein Eintrag vorhanden"
    }
  },
