  "pokedex_entries": {
    "X": {
      "en": "This very friendly Dragon-type Pokémon will hug its beloved Trainer, leaving that Trainer covered in sticky slime.",
      "de": "Dieses äußerst freundliche Drachen-Pokémon neigt dazu, seinen geliebten Trainer zu umarmen und so mit einer dicken Schleimschicht zu umhüllen."
    },
    "Y": {
      "en": "It attacks with retractable horns. It throws a punch that's the equivalent of the force of a hundred pro boxers.",
      "de": "Es greift den Gegner mit ausfahrbaren Fühlern an. Seine Stärke ist vergleichbar mit der Durchschlagskraft der Faustschläge von 100 Profiboxern."
    },
    "Omega Ruby": {
      "en": "This very friendly Dragon-type Pokémon will hug its beloved Trainer, leaving that Trainer covered in sticky slime.",
      "de": "Es greift den Gegner mit ausfahrbaren Fühlern an. Seine Stärke ist vergleichbar mit der Durchschlagskraft der Faustschläge von 100 Profiboxern."
    },
    "Alpha Sapphire": {
      "en": "It attacks with retractable horns. It throws a punch that's the equivalent of the force of a hundred pro boxers.",
      "de": "Es greift den Gegner mit ausfahrbaren Fühlern an. Seine Stärke ist vergleichbar mit der Durchschlagskraft der Faustschläge von 100 Profiboxern."
    },
    "Sun": {
      "en": "An amazingly friendly Pokémon, but if left to itself, loneliness overcomes it, and it oozes gooey tears.",
      "de": "Kein Eintrag vorhanden"
    },
    "Moon": {
      "en": "It gets picked on because it's meek. But then, whoever teased it gets to feel the full force of its horns and a good swatting from its thick tail.",
      "de": "Kein Eintrag vorhanden"
    }
